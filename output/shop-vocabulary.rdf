  <gr:QualitativeValue rdf:about="http://www.heppnetz.de/gr4shops/shop-vocabulary.html#V_XXL">
    <gr:greater>
      <gr:QualitativeValue rdf:about="http://www.heppnetz.de/gr4shops/shop-vocabulary.html#V_XL">
