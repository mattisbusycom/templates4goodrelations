        <gr:lesser>
          <gr:QualitativeValue rdf:about="http://www.heppnetz.de/gr4shops/shop-vocabulary.html#V_L">
            <gr:greater>
              <gr:QualitativeValue rdf:about="http://www.heppnetz.de/gr4shops/shop-vocabulary.html#V_M">
                <gr:greater>
                  <gr:QualitativeValue rdf:about="http://www.heppnetz.de/gr4shops/shop-vocabulary.html#V_S">
                    <gr:lesser rdf:resource="http://www.heppnetz.de/gr4shops/shop-vocabulary.html#V_M"/>
