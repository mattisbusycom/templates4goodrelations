                  </gr:QualitativeValue>
                </gr:greater>
                <gr:lesser rdf:resource="http://www.heppnetz.de/gr4shops/shop-vocabulary.html#V_L"/>
                <rdf:type>
                  <owl:Class rdf:about="http://www.heppnetz.de/gr4shops/shop-vocabulary.html#VC_shirt_sizes">
                    <rdfs:subClassOf rdf:resource="http://purl.org/goodrelations/v1#QualitativeValue"/>
                    <rdfs:label xml:lang="en">Garment sizes for shirts and sweaters (GoodRelations Product Feature Value Group)</rdfs:label>
                  </owl:Class>
                </rdf:type>
                <rdfs:label xml:lang="en">Garment size: Medium (M) (GoodRelations Product Feature Value)</rdfs:label>
              </gr:QualitativeValue>
            </gr:greater>
            <gr:lesser rdf:resource="http://www.heppnetz.de/gr4shops/shop-vocabulary.html#V_XL"/>
            <rdf:type rdf:resource="http://www.heppnetz.de/gr4shops/shop-vocabulary.html#VC_shirt_sizes"/>
            <rdfs:label xml:lang="en">Garment size: Large (L) (GoodRelations Product Feature Value)</rdfs:label>
          </gr:QualitativeValue>
        </gr:lesser>
        <rdf:type rdf:resource="http://www.heppnetz.de/gr4shops/shop-vocabulary.html#VC_shirt_sizes"/>
        <rdfs:label xml:lang="en">Garment size: Extra Large (XL) (GoodRelations Product Feature Value)</rdfs:label>
      </gr:QualitativeValue>
    </gr:greater>
    <rdf:type rdf:resource="http://www.heppnetz.de/gr4shops/shop-vocabulary.html#VC_shirt_sizes"/>
    <rdfs:label xml:lang="en">Garment size: XXL (GoodRelations Product Feature Value)</rdfs:label>
