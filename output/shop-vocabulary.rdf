      </owl:Class>
    </rdf:type>
    <rdfs:label xml:lang="en">Material: Genuine Leather (GoodRelations Product Feature Value)</rdfs:label>
  </gr:QualitativeValue>
  <owl:ObjectProperty rdf:about="http://www.heppnetz.de/gr4shops/shop-vocabulary.html#P_weight">
